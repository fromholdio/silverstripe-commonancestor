{
	"name": "fromholdio/silverstripe-commonancestor",
	"type": "silverstripe-vendormodule",
	"description": "A small utility class that accepts an array of class name, compares their class ancestories, and identifies their closest common ancestor class.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "hierarchy", "ancestor"],
	"homepage": "https://github.com/fromholdio/silverstripe-commonancestor",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-commonancestor/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
	"require": {
        "silverstripe/framework": "~4.0 || ~5.0"
	},
	"autoload": {
		"psr-4": {
			"Fromholdio\\CommonAncestor\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
